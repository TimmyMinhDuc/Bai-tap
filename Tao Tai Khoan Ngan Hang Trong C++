#include <iostream>
#include <string>

using namespace std;

class TaiKhoanNganHang {
private:
    string chuTaiKhoan;
    double soDuVND;
    const double QUY_DOI = 24613;

public:
    TaiKhoanNganHang(const string& chuTaiKhoan, double soDuVND) : chuTaiKhoan(chuTaiKhoan), soDuVND(soDuVND) {}

    string getChuTaiKhoan() const {
        return chuTaiKhoan;
    }

    double getSoDuVND() const {
        return soDuVND;
    }

    void deposit(double soLuongVND) {
        soDuVND += soLuongVND;
    }

    void withdraw(double soLuongVND) {
        if (soLuongVND > soDuVND) {
            cout << "Insufficient funds" << endl;
        } else {
            soDuVND -= soLuongVND;
        }
    }

    double getSoDuUSD() const {
        return soDuVND / QUY_DOI;
    }
};

int main() {
    TaiKhoanNganHang account("Minh DUc Bui", 1000000.0);

    cout << "Account owner: " << account.getChuTaiKhoan() << endl;
    cout << "Account balance: " << account.getSoDuVND() << " VND (" << account.getSoDuUSD() << " USD)" << endl;

    account.deposit(5000000);
    account.withdraw(350000);
    account.withdraw(200000);

    return 0;
}
